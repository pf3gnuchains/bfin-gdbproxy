static long double zeroL = 0.0L;
int main ()
{
  nocrash_init();
  if (sprintf (buf, "%Lf", 1.0L / 0.0L) < 0
      || (strcmp (buf, "inf") != 0 && strcmp (buf, "infinity") != 0))
    return 1;
  if (sprintf (buf, "%Lf", -1.0L / 0.0L) < 0
      || (strcmp (buf, "-inf") != 0 && strcmp (buf, "-infinity") != 0))
    return 1;
  if (sprintf (buf, "%Lf", zeroL / zeroL) < 0
      || !strisnan (buf, 0, strlen (buf)))
    return 1;
  if (sprintf (buf, "%Le", 1.0L / 0.0L) < 0
      || (strcmp (buf, "inf") != 0 && strcmp (buf, "infinity") != 0))
    return 1;
