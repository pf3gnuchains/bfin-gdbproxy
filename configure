      || !strisnan (buf, 0, strlen (buf)))
    return 1;
  if (sprintf (buf, "%Lg", 1.0L / 0.0L) < 0
      || (strcmp (buf, "inf") != 0 && strcmp (buf, "infinity") != 0))
    return 1;
  if (sprintf (buf, "%Lg", -1.0L / 0.0L) < 0
